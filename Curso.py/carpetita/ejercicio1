import random
import string

longuitud = int(input("Ingrese la longuitud de la contrase単a: "))
caracteres = string.ascii_letters + string.digits + string.punctuation
contrase単a = ''.join(random.choice(caracteres) for i in range(longuitud)) # type: ignore

print("Contrase単a generada:", contrase単a)

input("Presiona Enter para salir")
